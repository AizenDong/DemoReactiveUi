<?xml version="1.0" encoding="utf-8" ?>
<reactiveui:ReactiveContentPage
             x:TypeArguments="vm:CommandBindViewModel"
             xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="DemoReactiveUi.Pages.CommandBindPage"
             xmlns:vm="clr-namespace:DemoReactiveUi.ViewModels"
             xmlns:reactiveui="clr-namespace:ReactiveUI.XamForms;assembly=ReactiveUI.XamForms">
    <ContentPage.Content>
        <StackLayout>
            <Slider Minimum="0" Maximum="100" Value="50" x:Name="slider"/>
        </StackLayout>
    </ContentPage.Content>
</reactiveui:ReactiveContentPage>